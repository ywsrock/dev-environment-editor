{
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    },
   "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"]
    },
    "cSpell.userWords": [
      "Vundle"
    ]
  },
  "terraform": {
    "command": "terraform-lsp",
    "filetypes": ["terraform"],
    "initializationOptions": {}
  },
 "efm": {
    "command": "efm-langserver",
    "args": [],
    "filetypes": ["vim", "eruby", "markdown"]
  },
  "rust": {
    "command": "rust-analyzer",
    "filetypes": ["rust"],
    "rootPatterns": ["Cargo.toml"]
  },
"rubocop": {
    "command": "rubocop",
    "args": ["--lsp"],
    "filetypes": ["ruby"]
  },
  "lua": {
    "command": "lua-lsp",
    "filetypes": ["lua"]
  },
  "cSpell.userWords": [
    "whta"
  ],
  "snippets.ultisnips.pythonPrompt": false,
  "go.goplsOptions": {
    "completeUnimported": true,
    "analyses": {
      "fillreturns": true,
      "nonewvars": true,
      "staticcheck": true,
      "unusedparams": true,
      "unusedwrite": true,
      "unreachable": true,
      "httpresponse": true,
      "nilness": true,
      "nilfunc": true,
      "framepointer": true,
      "loopclosure": true,
      "waitgroup": true,
      "unmarshal": true,
      "timeformat": true,
      "stringintconv": true,
      "sortslice": true,
      "simplifyslice": true,
      "shadow": true,
      "printf": true,
      "noresultvalues": true,
      "modernize": true,
      "lostcancel": true,
      "ifaceassert": true,
      "errorsas": true,
      "defers": true,
      "copylocks": true,
      "composites": true,
      "atomicalign": true,
      "bools": true,
      "atomic": true,
      "appends": true,
      "gofumpt": true,
    }
  }
}
